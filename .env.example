# Google Sheets Configuration
# Copy this file to .env and replace with your actual values

# Your Google Sheets ID (found in the URL)
# Example: https://docs.google.com/spreadsheets/d/YOUR_SHEET_ID_HERE/edit
REACT_APP_GOOGLE_SHEETS_ID=your_google_sheets_id_here

# Optional: Sheet name (defaults to 'car-data-analytics')
REACT_APP_GOOGLE_SHEETS_NAME=car-data-analytics